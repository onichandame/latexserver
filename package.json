{
  "name": "latexserver",
  "version": "0.0.0",
  "description": "API server for compiling latex",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cd frontend && npm install",
