  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/onichandame/latexserver/issues"
  },
  "homepage": "https://github.com/onichandame/latexserver#readme",
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "fs-extra": "^8.1.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^2.0.2",
    "okitchen": "0.0.5",
    "unzipper": "^0.10.5"
  }
